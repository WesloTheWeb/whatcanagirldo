								<!-- Begin JavaScript -->
				<script src="http://code.jquery.com/jquery-latest.min.js"></script>
				<script>
				$(document).ready(function(){
					
					
					$('.more').on('click', function(){
						($(this).hasClass('open')) ? close() : open();
					});
					
				
					function open(){
						var count = $(".popup ul li").length;
						var total = count * 37 + 20;
						var top = total*-1;
						$('.popup').animate({
							height: total,
							marginTop: top
							});
						$('.more').animate({
							marginTop: top
						});
						$('.more').addClass('open');
						$('.more').removeClass('closed');
					}
					
					
					function close(){
						$('.popup').animate({
							height: 0,
							marginTop: "0"
						});
						$('.more').animate({
							marginTop: "0"
						});
						$('.more').addClass('closed');
						$('.more').removeClass('open');
					}
					
					});
				</script>
			<script src="http://code.jquery.com/jquery-1.11.1.min.js"></script>
		 
